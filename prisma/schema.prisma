generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  name     String
  password String
}

model Event {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  tipo     String
  email    String
  metadada Json
  createdAt DateTime @default(now())
}
